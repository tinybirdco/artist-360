
SCHEMA >
    `id` Int64,
    `external_id` String,
    `song_isrc` Nullable(String),
    `artists` String,
    `song_title` String,
    `song_title_version` Nullable(String),
    `album_name` Nullable(String),
    `plays` Int64

ENGINE "MergeTree"
ENGINE_PARTITION_KEY "substring(toString(id), 1, 1)"
ENGINE_SORTING_KEY "id, intHash32(plays)"
ENGINE_SAMPLING_KEY "intHash32(plays)"
